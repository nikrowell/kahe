{
  "name": "kahe",
  "version": "0.7.3",
  "description": "Minimal frontend framework for managing transition flow between application states.",
  "author": "Nik Rowell <nik@nikrowell.com>",
  "license": "MIT",
  "main": "kahe.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikrowell/kahe.git"
  },
  "keywords": [
    "browser",
    "frontend",
    "pushstate",
    "router",
    "transitions",
    "framework"
  ],
  "scripts": {
    "start": "rollup -c --watch",
    "bundle": "rollup -c",
    "build": "npm run bundle && uglifyjs kahe.js -mc --comments /^!/ > kahe.min.js && echo $(gzip -c kahe.min.js | wc -c)",
    "test": "budo test:bundle.js --live --open --pushstate --force-default-index -- -t [ babelify --presets [ es2015 ] ] --plugin tap-dev-tool",
    "version": "npm run build && git add --all"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "rollup": "^0.49.3",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-watch": "^4.3.1",
    "tap-dev-tool": "^1.3.0",
    "tape": "^4.8.0",
    "uglify-js": "^3.1.5"
  }
}
